@article{1748-0221-9-01-C01006,
  Abstract = {When the LHC resumes operation in 2015, the higher centre-of-mass energy and high-luminosity conditions will require significantly more sophisticated algorithms to select interesting physics events within the readout bandwidth limitations. The planned upgrade to the CMS calorimeter trigger will achieve this goal by implementing a flexible system based on the ÅÂºTCA standard, with modules based on Xilinx Virtex-7 FPGAs and up to 144 optical links running at speeds of 10 Gbps. The upgrade will improve the energy and position resolution of physics objects, enable much improved isolation criteria to be applied to electron and tau objects and facilitate pile-up subtraction to mitigate the effect of the increased number of interactions occurring in each bunch crossing. The design of the upgraded system is summarised with particular emphasis placed on the results of prototype testing and the experience gained which is of general application to the design of such systems.},
  Author = {M Baber and M Blake and J Brooke and M Cepeda Hermida and S Dasu and T Durkin and S Fayer and E K Friis and T Gorski and G Hall and K Harder and G Iles and S Ives and J Jones and P R Klabbers and A G Levine and C Lucas and R Lucas and D Newbold and J Marrouche and S Paramesvaran and T M Perry and A Rose and D Sankey and W Smith and A Tapper and A Thea and T Williams},
  Journal = {JINST},
  Number = {01},
  Pages = {C01006},
  Title = {Development and testing of an upgrade to the CMS level-1 calorimeter trigger},
  Url = {http://stacks.iop.org/1748-0221/9/i=01/a=C01006},
  Volume = {9},
  Year = {2014},
  }


    
@article{TMT_dem,
  author={R Frazier and S Fayer and G Hall and C Hunt and G Iles and D Newbold and A Rose},
  title={A demonstration of a Time Multiplexed Trigger for the CMS experiment},
  journal={JINST},
  volume={7},
  number={01},
  pages={C01060},
  url={http://stacks.iop.org/1748-0221/7/i=01/a=C01060},
  year={2012},
  abstract={A novel approach to first-level hardware triggering in the LHC experiments has been studied and a prototype system built. Calorimeter trigger primitive data ( ~ 5 Tb/s) are re-organised and time-multiplexed so that a single processing node (FPGA) may access the data corresponding to the entire detector for a given bunch crossing. This provides maximal flexibility in the construction of new trigger algorithms, which will be an important factor in ensuring adequate trigger performance at the very high levels of background expected at the upgraded LHC. A test system that incorporates all the key technologies for a final system and demonstrates the time-multiplexing and algorithm performance is presented.}
}
  
